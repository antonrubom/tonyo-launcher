{
    "manifest_version": 2,
    "name": "Tonyo Launcher",
    "version": "1.0.1",
    "description": "Quick launch Antony's Personal Media.",
    "icons": { "128": "icons/T_128.png" },
    "browser_action": {
        "defaut_icon": "icons/favicon-16x16.png",
        "default_popup": "extension.html"
    },
    "content_security_policy": "script-src 'self' https://kit.fontawesome.com/1e96facd7a.js; object-src 'self'",
    "permissions": ["activeTab"]
}